{
  "name": "@rybajs/system",
  "version": "0.0.1",
  "description": "Tools commonly used accros Ryba",
  "keywords": [
    "ryba",
    "nikita",
    "system",
    "authconfig"
  ],
  "author": "David Worms <david@adaltas.com>",
  "homepage": "http://www.ryba.io",
  "license": "MIT",
  "coffeelintConfig": {
    "indentation": {
      "level": "error",
      "value": 2
    },
    "line_endings": {
      "level": "error",
      "value": "unix"
    },
    "max_line_length": {
      "level": "ignore"
    }
  },
  "directories": {
    "lib": "lib",
    "test": "test"
  },
  "files": [
    "authconfig",
    "java"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://wdavidw@github.com/ryba-io/ryba.git"
  },
  "dependencies": {
    "@nikitajs/core": "^0.9.5",
    "each": "^1.2.2",
    "mixme": "^0.3.5"
  },
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "test": "echo \"Error: run tests from root\" && exit 1"
  },
  "bugs": {
    "url": "https://github.com/ryba-io/ryba/issues"
  }
}
